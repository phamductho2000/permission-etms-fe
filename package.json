{
    "name": "hcma-frontend",
    "version": "6.0.0",
    "private": true,
    "description": "HCMA Frontend",
    "scripts": {
        "analyze": "cross-env ANALYZE=1 max build",
        "build": "max build",
        "deploy": "npm run build && npm run gh-pages",
        "dev": "npm run start:dev",
        "gh-pages": "gh-pages -d dist",
        "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
        "jest": "jest",
        "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
        "lint-staged": "lint-staged",
        "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
        "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
        "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
        "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
        "codegen": "cross-env REACT_APP_ENV=dev UMI_ENV=dev max openapi",
        "codegen:test": "cross-env REACT_APP_ENV=test UMI_ENV=dev max openapi",
        "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
        "preview": "npm run build && max preview --port 8000",
        "record": "cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
        "serve": "umi-serve",
        "start": "cross-env REACT_APP_ENV=dev UMI_ENV=dev max dev",
        "start:dev": "cross-env REACT_APP_ENV=dev UMI_ENV=dev max dev",
        "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
        "start:test": "cross-env REACT_APP_ENV=test UMI_ENV=dev max dev",
        "start:prod": "cross-env REACT_APP_ENV=prod UMI_ENV=dev max dev",
        "test": "jest",
        "test:coverage": "npm run jest -- --coverage",
        "test:update": "npm run jest -- -u",
        "tsc": "tsc --noEmit"
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
        "**/*.{js,jsx,tsx,ts,less,md,json}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "dependencies": {
        "@ant-design/icons": "^4.8.1",
        "@ant-design/pro-components": "^2.6.28",
        "@types/file-saver": "^2.0.7",
        "@types/pdfmake": "^0.2.9",
        "@umijs/route-utils": "^2.2.2",
        "antd": "^5.12.7",
        "antd-style": "^3.5.2",
        "axios": "^1.6.4",
        "classnames": "^2.3.2",
        "dayjs": "^1.11.10",
        "deepdash": "^5.3.9",
        "deepdash-es": "^5.3.9",
        "exceljs": "^4.4.0",
        "file-saver": "^2.0.5",
        "jquery": "^3.7.1",
        "js-cookie": "^3.0.5",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "omit.js": "^2.0.2",
        "pdfmake": "^0.2.9",
        "querystring": "^0.2.1",
        "rc-menu": "^9.12.0",
        "rc-util": "^5.37.0",
        "react": "^18.2.0",
        "react-big-calendar": "^1.8.6",
        "react-dev-inspector": "^1.9.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^1.3.0",
        "url-loader": "^4.1.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@ant-design/pro-cli": "^2.1.5",
        "@testing-library/react": "^13.4.0",
        "@types/classnames": "^2.3.1",
        "@types/express": "^4.17.18",
        "@types/history": "^4.7.11",
        "@types/jest": "^29.5.5",
        "@types/jquery": "^3.5.29",
        "@types/lodash": "^4.14.199",
        "@types/lodash-es": "^4.17.12",
        "@types/react": "^18.2.25",
        "@types/react-big-calendar": "^1.8.8",
        "@types/react-dom": "^18.2.10",
        "@types/react-helmet": "^6.1.7",
        "@types/uuid": "^9.0.8",
        "@umijs/fabric": "^2.14.1",
        "@umijs/lint": "^4.0.83",
        "@umijs/max": "^4.0.83",
        "cross-env": "^7.0.3",
        "eslint": "^8.50.0",
        "express": "^4.18.2",
        "gh-pages": "^3.2.3",
        "husky": "^7.0.4",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^10.5.4",
        "prettier": "^2.8.8",
        "swagger-ui-dist": "^4.19.1",
        "tailwindcss": "^3.4.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2",
        "umi-presets-pro": "^2.0.3",
        "umi-serve": "^1.9.11"
    },
    "engines": {
        "node": ">=12.0.0"
    }
}
